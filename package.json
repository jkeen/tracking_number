{
  "name": "tracking_number",
  "version": "1.x.x-semantic-release",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkeen/tracking_number.git"
  },
  "author": "jkeen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkeen/tracking_number/issues"
  },
  "homepage": "https://github.com/jkeen/tracking_number#readme",
  "scripts": {
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@klueless-js/semantic-release-rubygem": "https://github.com/klueless-js/semantic-release-rubygem",
    "semantic-release": "^19.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1" 
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "*!",
              "release": "major"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES", "BREAKING"]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "tracking_number changelog",
          "changelogFile": "CHANGELOG.md"
        }
      ],
      ["@klueless-js/semantic-release-rubygem", {
        "gemHost": "https//rubygems.org",
        "updateGemfileLock": false,
        "gemPublish": true,
        "gemFileDir": false,
        "pkgRoot": "."
      }],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": ["CHANGELOG.md"],
          "message": "${nextRelease.version} CHANGELOG [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "debug": false,
    "dryRun": false
  }}
